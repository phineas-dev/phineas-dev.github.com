	<WorkExperienceDate start="Sep 1996" end="Jun 1999" />
	<p class="text-2xl">Leicester University, Leicester</p>
	<p class="text-2xl">Computer Science</p>

	<h2 class="text-3xl mt-8 mb-4">NOTABLE WORK</h2>
	<NotableWork imageProps={{ src:"/qualcomm.jpg" }}>
		<p>
			Complete redesign for a global technology brand <Link target="_blank" href="https://www.qualcomm.com">Qualcomm</Link>. Unified 6 different sites into a single React app
		</p>
	</NotableWork>
		
	<NotableWork imageProps={{ src:"/lennd.png" }}>
		<p>
			React and React Native app for the event management platform <Link target="_blank" href="https://www.lennd.com">Lennd</Link>, powered with GraphQL
		</p>
	</NotableWork>

	<NotableWork imageProps={{ src:"/monocle.jpg" }}>
		<p>
			Pixel perfect layouts for the global design magazine <Link target="_blank" href="https://www.monocle.com">Monocle</Link>
		</p>
	</NotableWork>
</DefaultLayout>
