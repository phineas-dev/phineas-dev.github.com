---
interface Props {
  start: string;
  end: string;
}

const { start, end } = Astro.props;
---

<div class="flex items-center gap-2 mb-2">
  <span class="text-sm font-medium px-2 py-1 bg-cyan-50 dark:bg-cyan-900/30 text-cyan-700 dark:text-cyan-300 rounded-md">
    {start} - {end}
  </span>
</div>
